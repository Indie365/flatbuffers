namespace keyfield.sample;

struct Baz {
  a: [uint8:4] (key); // A fixed-sized array of uint8 as a Key
  b: uint8 ;
}

struct Bar {
  a: [float:3] (key); // A fixed-sized array of float as a Key
  b: uint8;
}

struct Apple {
  a: uint8;
  b: Bar (key);
}

table FooTable {
  a: int;
  b: int;
  c: string (key);
  d: [Baz];
  e: [Bar];
  f: [Apple];
}
root_type FooTable;

