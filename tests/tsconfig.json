{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES2015", "ES2020.BigInt", "DOM"],
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "strict": false,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "noImplicitReturns": false,
    "strictNullChecks": false,
    "baseUrl": ".",
    "noEmit": false,
    "outDir": "./ts"
  },
  "include": [ "ts/**/*.ts" ],
  "exclude": []
}
